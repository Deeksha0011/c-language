#include <stdio.h>

int main() {
   char letter;
   printf("enter:");
   scanf("%c",&letter);
   if( letter=='a'|| letter=='A'|| letter=='e'|| letter=='E'||
   letter=='i'|| letter=='I'|| letter=='o'|| letter=='O'|| letter=='u'|| letter=='U')
   printf("vowel");
   else
   printf("consonant");

    return 0;
}
